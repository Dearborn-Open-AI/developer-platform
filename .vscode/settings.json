{
    "files.exclude": {
        "**/.vs": true,
        "**/bin": true,
        "**/obj": true,
        "**/.git": true,
        "**/.DS_Store": true,
        "**/**.egg-info": true,
        "**/__pycache__": true,
        "**/**.dist-info": true,
        "**/node_modules": true,
    },
    "explorer.fileNesting.patterns": {
        "*.cs": "I${capture}.cs, ${capture}JsonConverter.cs",
    },
    "markdownlint.config": {
        "default": true,
        "MD012": false,
        "MD009": false
    },
    "dotnet.defaultSolution": "Microsoft.Developer.sln",
    "omnisharp.projectFilesExcludePattern": "**/node_modules/**,**/.git/**,**/bower_components/**,**/.local/**",
    "debug.internalConsoleOptions": "neverOpen",
}