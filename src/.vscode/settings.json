{
    "azureFunctions.projectLanguage": "C#",
    "azureFunctions.projectRuntime": "~4",
    "files.exclude": {
        "**/.vs": true,
        "**/bin": true,
        "**/obj": true,
        "**/.git": true,
        "**/.DS_Store": true,
        "**/**.egg-info": true,
        "**/__pycache__": true,
        "**/**.dist-info": true,
        "**/node_modules": true,
    },
    "explorer.fileNesting.patterns": {
        "*.cs": "I${capture}.cs, I${capture}Factory.cs I ${capture}JsonConverter.cs",
        // "*.cs": "I${capture}.cs, ${capture}Spec.cs, ${capture}SpecConfig.cs, ${capture}Metadata.cs",
    },
    "markdownlint.config": {
        "default": true,
        "MD012": false,
        "MD009": false
    },
    "omnisharp.projectFilesExcludePattern": "**/node_modules/**,**/.git/**,**/bower_components/**,**/.local/**",
    "debug.internalConsoleOptions": "neverOpen",
    "dotnet.defaultSolution": "Microsoft.Developer.Platform.sln",
    "dotnet.preferRuntimeFromSDK": true
}